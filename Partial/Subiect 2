from hmmlearn import hmm
import numpy as np

p=np.array([.4,.3,.3])
t=np.array([
[.6,.3,.1],
[.2,.7,.1],
[.3,.2,.5]
])

e=np.array([
[.1,.7,.2],
[.05,.25,.7],
[.8,.15,.05]
])

m=hmm.CategoricalHMM(n_components=3)
m.startprob_=p
m.transmat_=t
m.emissionprob_=e
o=np.array([[1,2,0]]).T
x=m.score(o)
print(np.exp(x))
